.memo {
    height: 100%;
    width: 350px;
    display: flex;
    flex-flow: column nowrap;
    justify-content: space-between;
    align-items: left;
    .header{
      font-size: var(--font-header-size);
    }
    .text{
      font-size: var(--font-small-size);
      resize: none;
    }
}
